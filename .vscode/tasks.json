{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run Main Application",
			"type": "shell",
			"command": "python",
			"args": [
				"main.py"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"isBackground": false,
			"problemMatcher": [
				"$python"
			]
		},
		{
			"label": "Run Redis Examples",
			"type": "shell",
			"command": "python",
			"args": [
				"src/redis_examples.py"
			],
			"group": "build",
			"isBackground": false,
			"problemMatcher": [
				"$python"
			]
		},
		{
			"label": "Run Excel Examples",
			"type": "shell",
			"command": "python",
			"args": [
				"src/excel_examples.py"
			],
			"group": "build",
			"isBackground": false,
			"problemMatcher": [
				"$python"
			]
		},
		{
			"label": "Run Redis-Excel Integration",
			"type": "shell",
			"command": "python",
			"args": [
				"src/redis_excel_integration.py"
			],
			"group": "build",
			"isBackground": false,
			"problemMatcher": [
				"$python"
			]
		},
		{
			"label": "Install Dependencies",
			"type": "shell",
			"command": "pip",
			"args": [
				"install",
				"-r",
				"requirements.txt"
			],
			"group": "build",
			"isBackground": false,
			"problemMatcher": []
		}
	]
}